{
    "name": "frijj2k/auto-swagger-ui",
    "description": "Auto include Swagger-UI into Laravel project. Based on original source by Kevin Upton (uptonkg@gmail.com)",
    "keywords": [
        "API",
        "Documentation",
        "Laravel",
        "swagger-ui",
        "php"
    ],
    "type": "Extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Pearson",
            "email": "dave@pds-uk.com"
        },
        {
            "name": "Kevin Upton",
            "email": "uptonkg@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "laravel/framework": "~5.0",
        "laravel/lumen-framework": "~5.0",
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Frijj2k\\AutoSwaggerUI\\": "src/Frijj2k/AutoSwaggerUI"
        },
        "files": [
            "src/Frijj2k/AutoSwaggerUI/Helpers/helpers.php"
        ]
    }
}
